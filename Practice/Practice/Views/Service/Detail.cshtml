@model Service



<section id="services" class="services">
    <div class="container">

        <div class="row">

            <div class="col-lg-4 col-md-6 d-flex align-items-stretch mt-4">
                <div class="icon-box">
                    <div class="icon">@Html.Raw(Model.Icon)</div>
                    <h4><a asp-controller="Service" asp-action="Detail">@Model.Title</a></h4>
                    <p>@Model.Desc</p>
                </div>
            </div>


        </div>

    </div>
